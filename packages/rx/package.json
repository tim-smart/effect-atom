{
  "name": "@effect-rx/rx",
  "version": "0.5.2",
  "description": "Reactive toolkit for Effect",
  "main": "dist/effect-rx-rx.cjs.js",
  "module": "dist/effect-rx-rx.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/effect-ts/rx.git"
  },
  "homepage": "https://github.com/effect-ts/rx",
  "preconstruct": {
    "entrypoints": [
      "*.ts"
    ],
    "distFilenameStrategy": "full",
    "exports": {
      "importConditionDefaultExport": "default"
    }
  },
  "effect": {
    "generateIndex": true,
    "includeInternal": true
  },
  "scripts": {
    "example": "ts-node --project tsconfig.examples.json",
    "lint": "eslint --ext .ts,.tsx src/**/* test/**/*",
    "docs": "docgen",
    "tc": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "Effect contributors",
  "license": "MIT",
  "sideEffects": [],
  "devDependencies": {
    "effect": "2.0.0-next.45"
  },
  "peerDependencies": {
    "effect": "2.0.0-next.45"
  },
  "files": [
    "src",
    "dist",
    "internal",
    "Registry",
    "Result",
    "Rx",
    "RxRef"
  ],
  "exports": {
    ".": {
      "types": "./dist/declarations/src/index.d.ts",
      "module": "./dist/effect-rx-rx.esm.js",
      "import": "./dist/effect-rx-rx.cjs.mjs",
      "default": "./dist/effect-rx-rx.cjs.js"
    },
    "./package.json": "./package.json",
    "./Registry": {
      "types": "./dist/declarations/src/Registry.d.ts",
      "module": "./Registry/dist/effect-rx-rx-Registry.esm.js",
      "import": "./Registry/dist/effect-rx-rx-Registry.cjs.mjs",
      "default": "./Registry/dist/effect-rx-rx-Registry.cjs.js"
    },
    "./Result": {
      "types": "./dist/declarations/src/Result.d.ts",
      "module": "./Result/dist/effect-rx-rx-Result.esm.js",
      "import": "./Result/dist/effect-rx-rx-Result.cjs.mjs",
      "default": "./Result/dist/effect-rx-rx-Result.cjs.js"
    },
    "./Rx": {
      "types": "./dist/declarations/src/Rx.d.ts",
      "module": "./Rx/dist/effect-rx-rx-Rx.esm.js",
      "import": "./Rx/dist/effect-rx-rx-Rx.cjs.mjs",
      "default": "./Rx/dist/effect-rx-rx-Rx.cjs.js"
    },
    "./RxRef": {
      "types": "./dist/declarations/src/RxRef.d.ts",
      "module": "./RxRef/dist/effect-rx-rx-RxRef.esm.js",
      "import": "./RxRef/dist/effect-rx-rx-RxRef.cjs.mjs",
      "default": "./RxRef/dist/effect-rx-rx-RxRef.cjs.js"
    }
  }
}
